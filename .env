# isi dengan nomor yang ingin anda jadikan bot, kode pairing muncul di log
PAIRING_NUMBER="628XX"

# nomor owner untuk penggunaan command khusus owner
OWNER=["628XXX"]

# untuk size auto restart, RAM sisa 350MB auto restart
AUTO_RESTART=250 MB

# `true` untuk pribadi dan `false` untuk publik
SELF=true

# opsional
WRITE_STORE=true

#autocall
antiCall=true

# session name
SESSION_NAME=session


# opsional jikalau perlu, untuk backup status ke telegram
# token bot telegram bisa diambile ke https://t.me/BotFather
# ambil id telegrammu di https://t.me/getmyid_bot
TELEGRAM_TOKEN=
ID_TELEGRAM=
